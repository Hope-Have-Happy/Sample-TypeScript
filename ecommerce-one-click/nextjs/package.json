{
  "private": true,
  "name": "temporal-nextjs-oneclick",
  "scripts": {
    "dev": "temporal:build && npm run next:dev & npm run temporal:dev & npm run temporal:worker",
    "next:dev": "next dev",
    "temporal:dev": "tsc --build --watch ./temporal/tsconfig.json",
    "build": "next build",
    "temporal:build": "tsc --build ./temporal/tsconfig.json",
    "start": "echo 'use npm run dev instead'",
    "temporal:worker": "node ./temporal/lib/worker"
  },
  "dependencies": {
    "@headlessui/react": "^1.4.1",
    "@heroicons/react": "^1.0.4",
    "@tailwindcss/aspect-ratio": "^0.2.1",
    "@tailwindcss/forms": "^0.3.3",
    "next": "latest",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-toastify": "^8.0.3",
    "swr": "^1.0.1",
    "temporalio": "^0.4.2"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.2",
    "autoprefixer": "^10.2.6",
    "postcss": "^8.3.5",
    "tailwindcss": "^2.2.4",
    "typescript": "^4.4.3"
  }
}
